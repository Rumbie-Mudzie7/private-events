<h1> Sign In </h1>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :name%>
    <%= text_field_tag :name, params[:name] %>
  </p>

  <p>
    <%= label_tag :email%>
    <%= email_field_tag :email, params[:email] %>
  </p>

  <p class="button">
    <%= submit_tag "Sign In" %>
  </p>
<% end %>
